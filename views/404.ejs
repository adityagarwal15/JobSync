    /* Landscape orientation optimizations */
    @media (max-height: 600px) and (orientation: landscape) {
      body {
        padding: 0;
      }

      .error-container {
        min-height: auto;
        grid-template-columns: 1fr 1fr;
        gap: 2rem;
        padding: 1rem;
      }

      .visual-section {
        text-align: left;
        align-items: flex-start;
      }

      .error-number {
        font-size: clamp(4rem, 15vw, 6rem);
        margin-bottom: 0.5rem;
      }

      .content-card {
        padding: 1.5rem;
      }

      .help-section {
        grid-column: 1 / -1;
        margin-top: 1rem;
      }

      .help-card {
        padding: 1.5rem;
      }

      .help-grid {
        grid-template-columns: repeat(3, 1fr);
        gap: 1rem;
      }

      .floating-elements {
        display: none;
      }
    }

    /* High DPI displays optimizations */
    @media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
      .error-number,
      .btn,
      .help-link {
        transform: translateZ(0);
        backface-visibility: hidden;
      }
    }

    /* Accessibility improvements */
    @media (prefers-reduced-motion: reduce) {
      *, *::before, *::after {
        animation-duration: 0.01ms !important;
        animation-iteration-count: 1 !important;
        transition-duration: 0.01ms !important;
        scroll-behavior: auto !important;
      }
    }

    .btn:focus-visible,
    .help-link:focus-visible {
      outline: 2px solid var(--primary-orange);
      outline-offset: 3px;
    }

    /* Dark mode support */
    @media (prefers-color-scheme: dark) {
      :root {
        --bg-primary: #0a0a0a;
        --bg-secondary: #1a0505;
      }
    }

    /* Print styles */
    @media print {
      .background-system,
      .floating-elements,
      .particle-system {
        display: none;
      }
      
      body {
        background: white;
        color: black;
      }
      
      .error-number {
        color: black;
        -webkit-text-fill-color: black;
      }
    }
  </style>
</head>
<body>
  <div class="background-system">
    <div class="mesh-gradient"></div>
    <div class="gradient-orb orb-1"></div>
    <div class="gradient-orb orb-2"></div>
    <div class="gradient-orb orb-3"></div>
    <div class="particle-system" id="particleSystem"></div>
  </div>

  <main class="error-container">
    <section class="visual-section">
      <div class="floating-elements">
        <div class="floating-card floating-card-1"></div>
        <div class="floating-card floating-card-2"></div>
      </div>
      
      <div class="error-number-container">
        <div class="error-number" id="errorNumber">404</div>
      </div>
      <div class="visual-subtitle" id="visualSubtitle">Connection Lost</div>
    </section>

    <section class="content-section">
      <div class="content-card" id="contentCard">
        <h1 class="error-title" id="errorTitle">Page Not Found</h1>
        <p class="error-description" id="errorDescription">
          The page you're looking for has been moved, deleted, or doesn't exist. 
          This might happen when URLs change or when you follow an outdated link.
          <br><br>
          Error code: <span class="error-code">404_NOT_FOUND</span>
        </p>

        <div class="action-group">
          <a href="/" class="btn btn-primary" id="homeButton">
            Return to Dashboard
            <svg class="btn-icon" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"/>
              <polyline points="9,22 9,12 15,12 15,22"/>
            </svg>
          </a>
          <a href="/search" class="btn btn-secondary" id="searchButton">
            Search Jobs
            <svg class="btn-icon" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <circle cx="11" cy="11" r="8"/>
              <path d="m21 21-4.35-4.35"/>
            </svg>
          </a>
        </div>
      </div>
    </section>

     <section class="help-section">
      <div class="help-card" id="helpCard">
        <h2 class="help-title">Need assistance?</h2>
        <div class="help-links">
          <a href="/contact" class="help-link" data-help="contact">
            <svg class="help-icon" fill="currentColor" viewBox="0 0 24 24">
              <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 17h-2v-2h2v2zm2.07-7.75l-.9.92C13.45 12.9 13 13.5 13 15h-2v-.5c0-1.1.45-2.1 1.17-2.83l1.24-1.26c.37-.36.59-.86.59-1.41 0-1.1-.9-2-2-2s-2 .9-2 2H8c0-2.21 1.79-4 4-4s4 1.79 4 4c0 .88-.36 1.68-.93 2.25z"/>
            </svg>
            Contact Support
          </a>
          <a href="/sitemap" class="help-link" data-help="sitemap">
            <svg class="help-icon" fill="currentColor" viewBox="0 0 24 24">
              <path d="M9 3L5 6.99h3V14h2V6.99h3L9 3zm7 14.01V10h-2v7.01h-3L15 21l4-3.99h-3z"/>
            </svg>
            Browse Sitemap
          </a>
          <a href="/jobs" class="help-link" data-help="jobs">
            <svg class="help-icon" fill="currentColor" viewBox="0 0 24 24">
              <path d="M20 6h-2.5l-1.5-1.5h-5L9.5 6H7c-1.1 0-2 .9-2 2v10c0 1.1.9 2 2 2h13c1.1 0 2-.9 2-2V8c0-1.1-.9-2-2-2z"/>
            </svg>
            Latest Jobs
          </a>
        </div>
      </div>
    </section>
    

  </main>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/gsap.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/ScrollTrigger.min.js"></script>
  <script>
    // Register ScrollTrigger
    gsap.registerPlugin(ScrollTrigger);

    // Create particle system
    function createParticles() {
      const particleSystem = document.getElementById('particleSystem');
      const particleCount = window.innerWidth > 768 ? 50 : 25;
      
      for (let i = 0; i < particleCount; i++) {
        const particle = document.createElement('div');
        particle.className = 'particle';
        particle.style.left = Math.random() * 100 + '%';
        particle.style.animationDelay = Math.random() * 20 + 's';
        particle.style.animationDuration = (Math.random() * 10 + 10) + 's';
        particleSystem.appendChild(particle);
      }
    }

    // Advanced entrance sequence
    function initAnimations() {
      // Set initial states
      gsap.set([
        '.error-number',
        '.visual-subtitle',
        '.floating-card',
        '.content-card',
        '.help-card',
        '.help-item'
      ], { 
        opacity: 0, 
        y: 100,
        scale: 0.8
      });

      // Create master timeline
      const masterTL = gsap.timeline({
        defaults: { ease: "power4.out" }
      });

      // Staggered entrance animation
      masterTL
        .to('.error-number', {
          opacity: 1,
          y: 0,
          scale: 1,
          duration: 1.5,
          ease: "elastic.out(1, 0.5)"
        })
        .to('.visual-subtitle', {
          opacity: 1,
          y: 0,
          scale: 1,
          duration: 0.8
        }, "-=1")
        .to('.floating-card-1', {
          opacity: 0.6,
          y: 0,
          scale: 1,
          duration: 1,
          rotation: 5
        }, "-=0.8")
        .to('.floating-card-2', {
          opacity: 0.6,
          y: 0,
          scale: 1,
          duration: 1,
          rotation: -5
        }, "-=0.8")
        .to('.content-card', {
          opacity: 1,
          y: 0,
          scale: 1,
          duration: 1.2
        }, "-=0.6")
        .to('.help-card', {
          opacity: 1,
          y: 0,
          scale: 1,
          duration: 1
        }, "-=0.4")
        .from('.help-item', {
          opacity: 0,
          y: 50,
          scale: 0.9,
          duration: 0.6,
          stagger: 0.1
        }, "-=0.5");

      // Advanced hover effects
      document.querySelectorAll('.btn').forEach(btn => {
        const tl = gsap.timeline({ paused: true });
        
        tl.to(btn, {
          scale: 1.08,
          y: -8,
          duration: 0.3,
          ease: "power2.out"
        });

        btn.addEventListener('mouseenter', () => tl.play());
        btn.addEventListener('mouseleave', () => tl.reverse());
        
        // Click effect
        btn.addEventListener('click', () => {
          gsap.to(btn, {
            scale: 0.95,
            duration: 0.1,
            yoyo: true,
            repeat: 1,
            ease: "power2.inOut"
          });
        });
      });

      // Help items hover effects
      document.querySelectorAll('.help-link').forEach(item => {
        const tl = gsap.timeline({ paused: true });
        
        tl.to(link, {
          scale: 1.05,
          y: -10,
          duration: 0.3,
          ease: "power2.out"
        });

        item.addEventListener('mouseenter', () => tl.play());
        item.addEventListener('mouseleave', () => tl.reverse());
      });

      // Parallax mouse movement
      if (window.innerWidth > 1024) {
        let mouseX = 0, mouseY = 0;
        let targetX = 0, targetY = 0;

        document.addEventListener('mousemove', (e) => {
          mouseX = (e.clientX / window.innerWidth - 0.5) * 2;
          mouseY = (e.clientY / window.innerHeight - 0.5) * 2;
        });

        gsap.ticker.add(() => {
          targetX += (mouseX - targetX) * 0.05;
          targetY += (mouseY - targetY) * 0.05;

          gsap.set('.orb-1', {
            x: targetX * 50,
            y: targetY * 50
          });

          gsap.set('.orb-2', {
            x: targetX * -30,
            y: targetY * -30
          });

          gsap.set('.orb-3', {
            x: targetX * 20,
            y: targetY * 20
          });

          gsap.set('.floating-card-1', {
            x: targetX * 25,
            y: targetY * 25,
            rotation: targetX * 10
          });

          gsap.set('.floating-card-2', {
            x: targetX * -15,
            y: targetY * -15,
            rotation: targetX * -8
          });
        });
      }

      // Scroll-triggered animations
      ScrollTrigger.create({
        trigger: '.help-section',
        start: 'top 95%',
        end: 'bottom 60%',
        animation: gsap.to('.help-link', {
          y: -20,
          stagger: 0.1,
          ease: "none"
        }),
        scrub: 1
      });

      // Continuous subtle animations
      gsap.to('.orb-1', {
        rotation: 360,
        duration: 60,
        ease: "none",
        repeat: -1
      });

      gsap.to('.orb-2', {
        rotation: -360,
        duration: 80,
        ease: "none",
        repeat: -1
      });

      gsap.to('.orb-3', {
        scale: 1.1,
        duration: 10,
        ease: "power2.inOut",
        yoyo: true,
        repeat: -1
      });
    }

    // Initialize everything
    document.addEventListener('DOMContentLoaded', () => {
      createParticles();
      initAnimations();
    });

    // Handle window resize
    window.addEventListener('resize', () => {
      const particleSystem = document.getElementById('particleSystem');
      particleSystem.innerHTML = '';
      createParticles();
    });
  </script>
  </body>
</html>
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title>404 - Page Not Found | JOBSYNC</title>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=SF+Pro+Display:wght@300;400;500;600;700;800;900&family=Inter:wght@300;400;500;600;700&display=swap" rel="stylesheet">
  <style>
    :root {
      --primary-red: #fc002d;
      --primary-orange: #ff6b00;
      --accent-orange: #ff8534;
      --bg-primary: #0a0a0a;
      --bg-secondary: #1a0505;
      --text-primary: #ffffff;
      --text-secondary: #b8b8b8;
      --text-muted: #6a6a6a;
      --glass-bg: rgba(255, 255, 255, 0.05);
      --glass-border: rgba(255, 255, 255, 0.1);
      --glass-shadow: 0 8px 32px rgba(0, 0, 0, 0.3);
      --neon-glow: 0 0 20px rgba(252, 0, 45, 0.5);
    }

    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    body {
      font-family: 'SF Pro Display', -apple-system, BlinkMacSystemFont, 'Inter', sans-serif;
      background: var(--bg-primary);
      color: var(--text-primary);
      overflow-x: hidden;
      min-height: 100vh;
      display: flex;
      align-items: center;
      justify-content: center;
      -webkit-font-smoothing: antialiased;
      -moz-osx-font-smoothing: grayscale;
    }

    /* Advanced background system with particle effects */
    .background-system {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: -1;
      overflow: hidden;
    }

    .gradient-orb {
      position: absolute;
      border-radius: 50%;
      filter: blur(120px);
      opacity: 0.4;
      animation: orbFloat 25s ease-in-out infinite;
    }

    .orb-1 {
      width: 800px;
      height: 800px;
      background: radial-gradient(circle, var(--primary-red) 0%, transparent 70%);
      top: -30%;
      right: -20%;
      animation-delay: 0s;
    }

    .orb-2 {
      width: 600px;
      height: 600px;
      background: radial-gradient(circle, var(--primary-orange) 0%, transparent 70%);
      bottom: -20%;
      left: -15%;
      animation-delay: -5s;
      animation-direction: reverse;
    }

    .orb-3 {
      width: 400px;
      height: 400px;
      background: radial-gradient(circle, var(--accent-orange) 0%, transparent 70%);
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      animation-delay: -10s;
    }

    .mesh-gradient {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: 
        radial-gradient(circle at 20% 20%, rgba(252, 0, 45, 0.15) 0%, transparent 50%),
        radial-gradient(circle at 80% 80%, rgba(255, 107, 0, 0.12) 0%, transparent 50%),
        radial-gradient(circle at 40% 70%, rgba(255, 133, 52, 0.08) 0%, transparent 50%);
      animation: meshMove 15s ease-in-out infinite;
    }

    .particle-system {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      pointer-events: none;
    }

    .particle {
      position: absolute;
      width: 2px;
      height: 2px;
      background: var(--primary-orange);
      border-radius: 50%;
      opacity: 0.6;
      animation: particleFloat 20s linear infinite;
    }

    @keyframes orbFloat {
      0%, 100% { transform: translate(0, 0) scale(1) rotate(0deg); }
      25% { transform: translate(60px, -40px) scale(1.1) rotate(90deg); }
      50% { transform: translate(-40px, 60px) scale(0.9) rotate(180deg); }
      75% { transform: translate(30px, 30px) scale(1.05) rotate(270deg); }
    }

    @keyframes meshMove {
      0%, 100% { opacity: 0.3; transform: scale(1); }
      50% { opacity: 0.7; transform: scale(1.05); }
    }

    @keyframes particleFloat {
      0% { transform: translateY(100vh) translateX(0); opacity: 0; }
      10% { opacity: 0.6; }
      90% { opacity: 0.6; }
      100% { transform: translateY(-100px) translateX(100px); opacity: 0; }
    }

    /* Main container with advanced grid */
    .error-container {
      position: relative;
      max-width: 1400px;
      width: 100%;
      padding: 3rem;
      display: grid;
      grid-template-columns: 1.2fr 1fr;
      gap: 6rem;
      align-items: center;
      min-height: 85vh;
    }

    /* Visual section with 3D effects */
    .visual-section {
      display: flex;
      flex-direction: column;
      align-items: first baseline;
      justify-content: center;
      position: relative;
      perspective: 1000px;
    }

    .error-number-container {
      position: relative;
      margin-bottom: 2rem;
    }

    .error-number {
      font-family: 'SF Pro Display', sans-serif;
      font-size: clamp(10rem, 15vw, 20rem);
      font-weight: 800;
      line-height: 0.75;
      background: linear-gradient(135deg, 
        var(--primary-red) 0%, 
        var(--primary-orange) 30%, 
        var(--accent-orange) 60%, 
        var(--primary-red) 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
      position: relative;
      letter-spacing: -0.08em;
      transform-style: preserve-3d;
      animation: numberPulse 4s ease-in-out infinite;
    }

    .error-number::before {
      content: '404';
      position: absolute;
      top: 0;
      left: 0;
      z-index: -1;
      background: linear-gradient(135deg, var(--primary-red), var(--primary-orange));
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      filter: blur(20px);
      opacity: 0.5;
      animation: glowPulse 3s ease-in-out infinite;
    }

    .error-number::after {
      content: '';
      position: absolute;
      bottom: -15px;
      left: 0;
      width: 100%;
      height: 6px;
      background: linear-gradient(90deg, 
        transparent, 
        var(--primary-red), 
        var(--primary-orange), 
        var(--accent-orange), 
        transparent);
      border-radius: 3px;
      animation: underlineFlow 2s ease-in-out infinite;
    }

    .visual-subtitle {
      font-family: 'Inter', sans-serif;
      font-size: 1.125rem;
      font-weight: 500;
      color: var(--text-secondary);
      text-transform: uppercase;
      letter-spacing: 0.2em;
      margin-bottom: 3rem;
      opacity: 0.9;
      position: relative;
    }

    .visual-subtitle::before {
      content: '';
      position: absolute;
      left: -30px;
      top: 50%;
      width: 20px;
      height: 2px;
      background: linear-gradient(90deg, var(--primary-red), var(--primary-orange));
      transform: translateY(-50%);
    }

    .floating-elements {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      pointer-events: none;
    }

    .floating-card {
      position: absolute;
      background: var(--glass-bg);
      backdrop-filter: blur(30px);
      border: 1px solid var(--glass-border);
      border-radius: 20px;
      padding: 1.5rem;
      box-shadow: var(--glass-shadow);
      animation: floatCard 12s ease-in-out infinite;
    }

    .floating-card-1 {
      top: 20%;
      right: -10%;
      width: 120px;
      height: 80px;
      animation-delay: 0s;
    }

    .floating-card-2 {
      bottom: 30%;
      left: -15%;
      width: 100px;
      height: 100px;
      animation-delay: -4s;
    }

    @keyframes numberPulse {
      0%, 100% { transform: scale(1) rotateX(0deg); }
      50% { transform: scale(1.02) rotateX(5deg); }
    }

    @keyframes glowPulse {
      0%, 100% { opacity: 0.3; transform: scale(1); }
      50% { opacity: 0.7; transform: scale(1.05); }
    }

    @keyframes underlineFlow {
      0%, 100% { transform: translateX(-100%); opacity: 0; }
      50% { transform: translateX(0); opacity: 1; }
    }

    @keyframes floatCard {
      0%, 100% { transform: translateY(0) rotate(0deg); }
      25% { transform: translateY(-20px) rotate(5deg); }
      75% { transform: translateY(20px) rotate(-5deg); }
    }

    /* Content section with advanced glassmorphism */
    .content-section {
      display: flex;
      flex-direction: column;
      justify-content: center;
      gap: 3rem;
    }

    .content-card {
      background: var(--glass-bg);
      backdrop-filter: blur(40px);
      border: 1px solid var(--glass-border);
      border-radius: 32px;
      padding: 4rem;
      box-shadow: 
        var(--glass-shadow),
        inset 0 1px 0 rgba(255, 255, 255, 0.1);
      position: relative;
      overflow: hidden;
    }

    .content-card::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      height: 1px;
      background: linear-gradient(90deg, 
        transparent, 
        rgba(255, 255, 255, 0.3), 
        transparent);
    }

    .error-title {
      font-family: 'SF Pro Display', sans-serif;
      font-size: clamp(3rem, 5vw, 4.5rem);
      font-weight: 800;
      line-height: 1.1;
      color: var(--text-primary);
      margin-bottom: 2rem;
      letter-spacing: -0.03em;
      position: relative;
    }

    .error-description {
      font-family: 'Inter', sans-serif;
      font-size: 1.25rem;
      line-height: 1.8;
      color: var(--text-secondary);
      margin-bottom: 3rem;
      font-weight: 400;
    }

    .error-code {
      display: inline-block;
      background: rgba(252, 0, 45, 0.1);
      color: var(--primary-red);
      padding: 0.5rem 1rem;
      border-radius: 8px;
      font-family: 'SF Mono', monospace;
      font-size: 0.9rem;
      margin: 0 0.25rem;
    }

    /* Advanced action buttons */
    .action-group {
      display: flex;
      flex-direction: column;
      gap: 1.5rem;
    }

    .btn {
      position: relative;
      display: inline-flex;
      align-items: center;
      justify-content: center;
      padding: 1.5rem 3rem;
      border-radius: 20px;
      font-family: 'SF Pro Display', sans-serif;
      font-weight: 700;
      font-size: 1.125rem;
      text-decoration: none;
      transition: all 0.4s cubic-bezier(0.23, 1, 0.32, 1);
      border: none;
      cursor: pointer;
      overflow: hidden;
      backdrop-filter: blur(20px);
      position: relative;
    }

    .btn::before {
      content: '';
      position: absolute;
      top: 0;
      left: -100%;
      width: 100%;
      height: 100%;
      background: linear-gradient(90deg, 
        transparent, 
        rgba(255, 255, 255, 0.2), 
        transparent);
      transition: left 0.6s cubic-bezier(0.23, 1, 0.32, 1);
    }

    .btn:hover::before {
      left: 100%;
    }

    .btn-primary {
      background: linear-gradient(135deg, 
        var(--primary-red), 
        var(--primary-orange), 
        var(--accent-orange));
      color: white;
      box-shadow: 
        0 20px 60px rgba(252, 0, 45, 0.4),
        inset 0 1px 0 rgba(255, 255, 255, 0.2);
      border: 1px solid rgba(255, 255, 255, 0.1);
    }

    .btn-primary:hover {
      transform: translateY(-6px) scale(1.05);
      box-shadow: 
        0 30px 80px rgba(252, 0, 45, 0.6),
        inset 0 1px 0 rgba(255, 255, 255, 0.3);
    }

    .btn-secondary {
      background: var(--glass-bg);
      border: 1px solid var(--glass-border);
      color: var(--text-primary);
      box-shadow: 
        0 12px 40px rgba(0, 0, 0, 0.3),
        inset 0 1px 0 rgba(255, 255, 255, 0.05);
    }

    .btn-secondary:hover {
      background: rgba(255, 255, 255, 0.1);
      border-color: rgba(255, 107, 0, 0.4);
      transform: translateY(-4px) scale(1.02);
      box-shadow: 
        0 20px 60px rgba(0, 0, 0, 0.4),
        inset 0 1px 0 rgba(255, 255, 255, 0.1);
    }

    .btn-icon {
      margin-left: 0.5rem;
      transition: transform 0.3s ease;
    }

    .btn:hover .btn-icon {
      transform: translateX(5px);
    }

    /* Enhanced help section with glassmorphism */
    .help-section {
      grid-column: 1 / -1;
      margin-top: 4rem;
    }

    .help-card {
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      border: 1px solid var(--glass-border);
      border-radius: 24px;
      padding: 2.5rem;
      box-shadow: var(--glass-shadow);
    }

    .help-title {
      font-family: 'SF Pro Display', sans-serif;
      font-size: 1.5rem;
      font-weight: 600;
      margin-bottom: 1.5rem;
      color: var(--text-primary);
      letter-spacing: -0.01em;
    }

    .help-links {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
      gap: 1rem;
    }

    .help-link {
      display: flex;
      align-items: center;
      padding: 1.25rem;
      background: rgba(255, 255, 255, 0.03);
      border: 1px solid rgba(255, 255, 255, 0.06);
      border-radius: 16px;
      text-decoration: none;
      color: var(--text-secondary);
      transition: all 0.3s cubic-bezier(0.23, 1, 0.32, 1);
      backdrop-filter: blur(10px);
      font-family: 'Inter', sans-serif;
      font-weight: 500;
    }

    .help-link:hover {
      background: rgba(255, 255, 255, 0.06);
      border-color: rgba(255, 107, 0, 0.25);
      color: var(--text-primary);
      transform: translateY(-2px);
      box-shadow: 0 8px 32px rgba(255, 107, 0, 0.1);
    }

    .help-icon {
      width: 22px;
      height: 22px;
      margin-right: 1rem;
      opacity: 0.8;
      transition: all 0.3s ease;
    }

    .help-link:hover .help-icon {
      opacity: 1;
      transform: scale(1.1);
    }

      

    /* Ultra-responsive design */
    
    /* Ultra-wide screens (1920px+) */
    @media (min-width: 1920px) {
      .error-container {
        max-width: 1800px;
        gap: 10rem;
        padding: 5rem;
      }
      
      .content-card {
        padding: 5rem;
      }
      
      .help-card {
        padding: 4rem;
      }
    }

    /* Large Desktop (1440px - 1919px) */
    @media (min-width: 1440px) and (max-width: 1919px) {
      .error-container {
        gap: 8rem;
        padding: 4rem;
      }
    }

    /* Desktop (1200px - 1439px) */
    @media (min-width: 1200px) and (max-width: 1439px) {
      .error-container {
        gap: 6rem;
        padding: 3rem;
      }
    }

    /* Small Desktop (1024px - 1199px) */
    @media (min-width: 1024px) and (max-width: 1199px) {
      .error-container {
        gap: 4rem;
        padding: 2.5rem;
      }
      
      .content-card {
        padding: 3rem;
      }
    }

    /* Tablet Landscape (768px - 1023px) */
    @media (max-width: 1023px) and (min-width: 768px) {
      .error-container {
        grid-template-columns: 1fr;
        gap: 4rem;
        text-align: center;
        padding: 2rem;
        max-width: 900px;
      }

      .visual-section {
        align-items: center;
        order: 1;
      }

      .content-section {
        order: 2;
      }

      .help-section {
        order: 3;
        margin-top: 3rem;
      }

      .floating-elements {
        display: none;
      }

      .error-number {
        font-size: clamp(8rem, 25vw, 12rem);
      }

      .help-grid {
        grid-template-columns: 1fr;
        gap: 1.5rem;
      }
    }

    /* Tablet Portrait (601px - 767px) */
    @media (max-width: 767px) and (min-width: 601px) {
      body {
        padding: 1rem 0;
      }

      .error-container {
        grid-template-columns: 1fr;
        gap: 3rem;
        padding: 1.5rem;
        text-align: center;
        min-height: auto;
      }

      .visual-section {
        align-items: center;
      }

      .error-number {
        font-size: clamp(6rem, 30vw, 9rem);
        margin-bottom: 1rem;
      }

      .visual-subtitle {
        font-size: 1.1rem;
        margin-bottom: 2rem;
      }

      .content-card {
        padding: 2.5rem;
        border-radius: 24px;
      }

      .error-title {
        font-size: clamp(2.2rem, 7vw, 3rem);
      }

      .error-description {
        font-size: 1.1rem;
        margin-bottom: 2.5rem;
      }

      .btn {
        padding: 1.2rem 2.5rem;
        font-size: 1rem;
      }

      .help-card {
        padding: 2.5rem;
      }

      .help-grid {
        grid-template-columns: 1fr;
        gap: 1.5rem;
      }

      .floating-elements {
        display: none;
      }
    }

    /* Mobile Large (481px - 600px) */
    @media (max-width: 600px) and (min-width: 481px) {
      body {
        padding: 0.5rem 0;
      }

      .error-container {
        gap: 2.5rem;
        padding: 1rem;
        text-align: center;
        min-height: 100vh;
      }

      .error-number {
        font-size: clamp(5rem, 35vw, 7rem);
        margin-bottom: 0.5rem;
      }

      .error-number::after {
        height: 4px;
        bottom: -10px;
      }

      .visual-subtitle {
        font-size: 1rem;
        letter-spacing: 0.2em;
        margin-bottom: 1.5rem;
      }

      .content-card {
        padding: 2rem;
        border-radius: 20px;
      }

      .error-title {
        font-size: clamp(1.8rem, 8vw, 2.5rem);
        margin-bottom: 1rem;
      }

      .error-description {
        font-size: 1rem;
        line-height: 1.7;
        margin-bottom: 2rem;
      }

      .action-group {
        gap: 1rem;
      }

      .btn {
        padding: 1rem 2rem;
        font-size: 0.95rem;
        border-radius: 16px;
      }

      .help-section {
        margin-top: 2rem;
      }

      .help-card {
        padding: 2rem;
        border-radius: 20px;
      }

      .help-title {
        font-size: 1.5rem;
        margin-bottom: 1.5rem;
      }

      .help-grid {
        gap: 1rem;
      }

      .help-link{
        padding: 2rem;
        border-radius: 16px;
      }

      .floating-elements {
        display: none;
      }
    }

    /* Mobile Medium (376px - 480px) */
    @media (max-width: 480px) and (min-width: 376px) {
      body {
        padding: 0.25rem 0;
      }

      .error-container {
        gap: 2rem;
        padding: 0.75rem;
        min-height: 100vh;
      }

      .error-number {
        font-size: clamp(4rem, 40vw, 6rem);
        margin-bottom: 0.5rem;
      }

      .error-number::after {
        height: 3px;
        bottom: -8px;
      }

      .visual-subtitle {
        font-size: 0.9rem;
        letter-spacing: 0.15em;
        margin-bottom: 1rem;
      }

      .content-card {
        padding: 1.5rem;
        border-radius: 16px;
      }

      .error-title {
        font-size: clamp(1.6rem, 9vw, 2.2rem);
        margin-bottom: 0.75rem;
        line-height: 1.2;
      }

      .error-description {
        font-size: 0.95rem;
        line-height: 1.6;
        margin-bottom: 1.5rem;
      }

      .btn {
        padding: 0.9rem 1.8rem;
        font-size: 0.9rem;
        border-radius: 14px;
      }

      .help-card {
        padding: 1.5rem;
        border-radius: 16px;
      }

      .help-title {
        font-size: 1.3rem;
        margin-bottom: 1rem;
      }

      .help-link {
        padding: 1.25rem;
        border-radius: 14px;
      }

      .help-icon {
        width: 24px;
        height: 24px;
      }
    }

    /* Mobile Small (320px - 375px) */
    @media (max-width: 375px) {
      body {
        padding: 0.125rem 0;
      }

      .error-container {
        gap: 1.5rem;
        padding: 0.5rem;
        min-height: 100vh;
      }

      .error-number {
        font-size: clamp(3.5rem, 45vw, 5rem);
        margin-bottom: 0.25rem;
      }

      .error-number::after {
        height: 2px;
        bottom: -6px;
      }

      .visual-subtitle {
        font-size: 0.8rem;
        letter-spacing: 0.1em;
        margin-bottom: 0.75rem;
      }

      .content-card {
        padding: 1.25rem;
        border-radius: 14px;
      }

      .error-title {
        font-size: clamp(1.4rem, 10vw, 2rem);
        margin-bottom: 0.5rem;
      }

      .error-description {
        font-size: 0.9rem;
        line-height: 1.5;
        margin-bottom: 1.25rem;
      }

      .btn {
        padding: 0.8rem 1.5rem;
        font-size: 0.85rem;
        border-radius: 12px;
      }

      .help-section {
        margin-top: 1.5rem;
      }

      .help-card {
        padding: 1.25rem;
        border-radius: 14px;
      }

      .help-title {
        font-size: 1.2rem;
        margin-bottom: 0.75rem;
      }

      .help-link {
        padding: 1rem;
        border-radius: 12px;
      }

      .help-icon {
        width: 20px;
        height: 20px;
        margin-right: 0.75rem;
      }
    }

    /* Ultra-small devices (below 320px) */
    @media (max-width: 319px) {
      .error-container {
        gap: 1rem;
        padding: 0.25rem;
      }

      .error-number {
        font-size: clamp(3rem, 50vw, 4rem);
      }

      .content-card,
      .help-card {
        padding: 1rem;
        border-radius: 12px;
      }

      .error-title {
        font-size: clamp(1.2rem, 12vw, 1.8rem);
      }

      .error-description {
        font-size: 0.85rem;
      }

      .btn {
        padding: 0.7rem 1.2rem;
        font-size: 0.8rem;
      }
    }

    /* Foldable devices */
    @media (max-width: 280px) {
      .error-container {
        padding: 0.125rem;
      }

      .error-number {
        font-size: clamp(2.5rem, 55vw, 3.5rem);
      }

      .content-card {
        padding: 0.75rem;
      }

      .btn {
        padding: 0.6rem 1rem;
        font-size: 0.75rem;
      }
    }


